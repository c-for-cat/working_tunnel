{
  "name": "subdomain-proxy-service",
  "version": "1.0.0",
  "description": "A proxy service that allows dynamic creation of subdomain proxies",
  "main": "Proxy.js",
  "scripts": {
    "start": "node Proxy.js",
    "dev": "nodemon Proxy.js",
    "keep-alive": "node keep-alive.js",
    "start:all": "concurrently \"npm run start\" \"npm run keep-alive\""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "mongoose": "^7.5.0",
    "node-fetch": "^2.6.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
} 